@page
@model OnlineShop.Pages.MoreDetailsModel
@{
}
<h2>Product Detailes</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Title</th>
            <th scope="col">Description</th>
            <th scope="col">Price</th>
            <th scope="col">Color</th>
            <th scope="col">categoryId</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.Product.Id</th>
            <td>@Model.Product.Name</td>
            <td>@Model.Product.Title</td>
            <td>@Model.Product.Description</td>
            <td>@Model.Product.Price</td>
            <td>@Model.Product.Color</td>
            <td>@Model.Product.CategoryId</td>
        </tr>
</table>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}